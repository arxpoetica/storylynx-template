    "cross-fetch": "3.0.3",
    "dayjs": "1.8.14",
    "esm": "3.2.25",
    "express": "4.17.1",
    "helmet": "3.18.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "sirv": "0.4.2"
  },
