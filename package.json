{
  "name": "storylynx-template",
  "description": "Tell your story through the hyperlynx of the web.",
  "version": "0.0.1",
  "license": "MIT",
  "volta": {
    "node": "13.14.0"
  },
  "scripts": {
    "lynx-init": "dotenv lynx init",
    "dev": "sapper dev",
    "build": "trim build --postcss --env=.env && sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "debug": "sapper dev --inspect",
    "debug:build": "node --inspect --inspect-brk ./node_modules/sapper/sapper build --legacy",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=yourkeyring --key=yourkey",
    "deploy": "gcloud builds submit --config=cloudbuild.yaml .",
    "bake": "run-s keyring deploy",
    "watch": "trim watch --svgo --postcss --env=.env"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "helmet": "3.23.3",
    "morgan": "1.10.0",
    "sirv": "1.0.1",
    "storylynx": "git+https://022f228b4bbef5f76b885af420904c6cddfd2d70:x-oauth-basic@github.com/arxpoetica/storylynx.git"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/runtime": "7.10.4",
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@rollup/plugin-replace": "2.3.3",
    "@sapper-dragon/postcss": "0.1.4",
    "@sapper-dragon/svgo": "0.1.4",
    "@sapper-dragon/trimmings": "0.1.4",
    "chokidar": "3.4.0",
    "dotenv-cli": "3.2.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.18.2",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "6.1.0",
    "sapper": "0.27.16",
    "svelte": "3.23.2"
  }
}
