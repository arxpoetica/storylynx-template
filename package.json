{
  "name": "storylynx",
  "description": "Tell your story through the hyperlynx of the web.",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "debug": "sapper dev --inspect",
    "debug:build": "node --inspect --inspect-brk ./node_modules/sapper/sapper build --legacy",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=yourkeyring --key=yourkey",
    "publish": "gcloud builds submit --config=cloudbuild.yaml .",
    "deploy": "beta run deploy yourproject --image gcr.io/arxpoetica/yourproject --region us-central1",
    "bake": "run-s build publish deploy",
    "watch": "trim watch --svgo --postcss --json"
  },
  "dependencies": {
    "@arxpoetica/utils": "0.0.2",
    "ansi-colors": "4.1.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cross-fetch": "3.0.4",
    "crypto-js": "3.1.9-1",
    "dayjs": "1.8.17",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "helmet": "3.21.2",
    "jsonwebtoken": "8.5.1",
    "lorem-ipsum": "2.0.3",
    "morgan": "1.9.1",
    "quill": "1.3.7",
    "sirv": "0.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/runtime": "7.7.4",
    "@sapper-dragon/json": "0.0.10",
    "@sapper-dragon/postcss": "0.0.10",
    "@sapper-dragon/svgo": "0.0.10",
    "@sapper-dragon/trimmings": "0.0.10",
    "cheerio": "1.0.0-rc.3",
    "npm-run-all": "^4.1.5",
    "rollup": "1.27.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "5.1.1",
    "rollup-plugin-terser": "5.1.2",
    "sapper": "0.27.9",
    "svelte": "3.15.0"
  }
}
