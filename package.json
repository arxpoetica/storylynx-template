  "scripts": {
    "dev": "sapper dev",
    "build": "npx sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "debug": "sapper dev --inspect",
    "debug:build": "node --inspect --inspect-brk ./node_modules/sapper/sapper build --legacy",
    "dev:generate:news": "node -r esm -r ./src/server/build/config.js scripts/news-generate-fake-content.js",
    "dev:fix:news": "node -r esm -r ./src/server/build/config.js scripts/news-fix-fake-content.js",
    "watch": "trim watch --svgo --postcss --json"
  },
  "dependencies": {
    "ansi-colors": "3.2.4",
