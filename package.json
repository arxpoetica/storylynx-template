{
  "name": "storylynx-template",
  "description": "Tell your story through the hyperlynx of the web.",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "volta": {
    "node": "16.3.0"
  },
  "scripts": {
    "lynx-init": "dotenv lynx init",
    "dev": "dotenv svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=yourkeyring --key=yourkey",
    "deploy": "gcloud builds submit --config=cloudbuild.yaml .",
    "bake": "run-s keyring deploy",
    "cors": "gsutil cors set cors.json gs://storylynx-asset-loader",
    "cleanup": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm install",
    "update": "pnpm update --interactive --latest"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "dotenv": "10.0.0",
    "esm": "3.2.25",
    "storylynx": "arxpoetica/storylynx",
    "storylynx-api-graphcms": "arxpoetica/storylynx-api-graphcms"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.1",
    "dotenv-cli": "4.0.0",
    "npm-run-all": "4.1.5",
    "svelte": "3.38.2",
    "svelte-check": "2.1.0",
    "svelte-preprocess": "4.7.3",
    "tslib": "2.3.0",
    "typescript": "4.3.2"
  }
}
