{
  "name": "storylynx-template",
  "description": "Tell your story through the hyperlynx of the web.",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "volta": {
    "node": "16.3.0"
  },
  "scripts": {
    "lynx-init": "dotenv lynx init",
    "dev": "dotenv svelte-kit dev",
    "watch": "dotenv node -r esm src/build/watch.js",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=yourkeyring --key=yourkey",
    "deploy": "gcloud builds submit --config=cloudbuild.yaml .",
    "bake": "run-s keyring deploy",
    "cors": "gsutil cors set cors.json gs://storylynx-asset-loader",
    "cleanup": "rm -rf node_modules && rm pnpm-lock.yaml && pnpm install",
    "update": "pnpm update --interactive --latest",
    "postinstall": "node src/build/post-install.js"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "storylynx": "link:../storylynx",
    "storylynx-api-graphcms": "link:../storylynx-api-graphcms"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.0.0-next.115",
    "cheap-watch": "1.0.3",
    "dotenv-cli": "4.0.0",
    "esm": "3.2.25",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.2",
    "postcss-custom-media": "7.0.8",
    "postcss-easy-import": "3.0.0",
    "postcss-global-nested": "1.2.0",
    "postcss-hexrgba": "2.0.1",
    "postcss-load-config": "3.0.1",
    "postcss-math": "0.0.10",
    "postcss-media-minmax": "4.0.0",
    "postcss-mixins": "6.2.3",
    "postcss-nested": "4.2.3",
    "postcss-reporter": "6.0.1",
    "postcss-scss": "3.0.5",
    "postcss-simple-vars": "5.0.2",
    "postcss-strip-inline-comments": "0.1.5",
    "svelte": "3.38.2",
    "svelte-check": "2.1.0",
    "svelte-preprocess": "4.7.3",
    "svgo": "1.3.2",
    "tslib": "2.3.0",
    "typescript": "4.3.2"
  }
}
