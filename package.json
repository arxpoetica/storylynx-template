{
  "name": "storylynx-template",
  "description": "Tell your story through the hyperlynx of the web.",
  "version": "0.0.1",
  "license": "MIT",
  "volta": {
    "node": "13.14.0"
  },
  "scripts": {
    "lynx-init": "dotenv lynx init",
    "dev": "sapper dev",
    "build": "trim build --postcss --env=.env && sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "debug": "sapper dev --inspect",
    "debug:build": "node --inspect --inspect-brk ./node_modules/sapper/sapper build --legacy",
    "keyring": "gcloud kms encrypt --plaintext-file=.env.enc-source --ciphertext-file=.env.enc --location=global --keyring=yourkeyring --key=yourkey",
    "deploy": "gcloud builds submit --config=cloudbuild.yaml .",
    "bake": "run-s keyring deploy",
    "watch": "trim watch --svgo --postcss --env=.env"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "esm": "3.2.25",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "morgan": "1.10.0",
    "sirv": "0.4.2",
    "storylynx": "git+https://git@github.com/arxpoetica/storylynx.git"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/runtime": "7.9.6",
    "@rollup/plugin-babel": "5.0.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "4.0.3",
    "@rollup/plugin-node-resolve": "7.1.3",
    "@rollup/plugin-replace": "2.3.2",
    "@sapper-dragon/postcss": "0.1.3",
    "@sapper-dragon/svgo": "0.1.0",
    "@sapper-dragon/trimmings": "0.1.3",
    "chokidar": "3.4.0",
    "dotenv-cli": "3.1.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.7.6",
    "rollup-plugin-svelte": "5.2.1",
    "rollup-plugin-terser": "5.3.0",
    "sapper": "0.27.12",
    "svelte": "3.22.1"
  }
}
